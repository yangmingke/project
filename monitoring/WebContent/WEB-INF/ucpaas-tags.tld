<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<taglib xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.0"
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-jsptaglibrary_2_0.xsd">
	<description><![CDATA[ucpaas自定义标签]]></description>
	<display-name>ucpaas Tags</display-name>
	<tlib-version>1.0</tlib-version>
	<short-name>u</short-name>
	<uri>/ucpaas-tags</uri>

	<tag>
		<description><![CDATA[分页标签]]></description>
		<name>page</name>
		<tag-class>com.network.monitor.tag.PageTag</tag-class>
		<body-content>JSP</body-content>
		<attribute>
			<description><![CDATA[分页查询容器PageContainer]]></description>
			<name>page</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[查询的表单id]]></description>
			<name>formId</name>
			<required>true</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<dynamic-attributes>false</dynamic-attributes>
	</tag>

	<tag>
		<description><![CDATA[下拉框标签，三种使用方式：1.设置dictionaryType；2.设置sqlId；3.设置data]]></description>
		<name>select</name>
		<tag-class>com.network.monitor.tag.SelectTag</tag-class>
		<body-content>JSP</body-content>
		<attribute>
			<description><![CDATA[html的id、name标记]]></description>
			<name>id</name>
			<required>true</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[字典类型，即tb_ucpaas_params.param_type字段]]></description>
			<name>dictionaryType</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[包含的值，多个值用,分割]]></description>
			<name>includeValue</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[排除的值，多个值用,分割]]></description>
			<name>excludeValue</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[查询sql的id，即tagMapper.xml中的id]]></description>
			<name>sqlId</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[sql参数值，json格式]]></description>
			<name>sqlParams</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[直接传入的数据，json格式，如[{value:'1',text:'文本1'},{value:'2',text:'文本2'}]]]></description>
			<name>data</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[选中的值]]></description>
			<name>value</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[占位符]]></description>
			<name>placeholder</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[选择后的回调函数，3个参数value、text、isInit（是否初始化）]]></description>
			<name>callback</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[添加样式class]]></description>
			<name>clazz</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[添加样式]]></description>
			<name>style</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<dynamic-attributes>false</dynamic-attributes>
	</tag>

