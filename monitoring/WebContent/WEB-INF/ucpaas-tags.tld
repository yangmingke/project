			<name>sqlId</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[sql参数值，json格式]]></description>
			<name>sqlParams</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[直接传入的数据，json格式，如[{value:'1',text:'文本1'},{value:'2',text:'文本2'}]]]></description>
			<name>data</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
		</attribute>
		<attribute>
			<description><![CDATA[选中的值]]></description>
			<name>value</name>
